    "pre-push": "^0.1.1",
    "sqlite3": "^4.0.9",
    "tslint": "^5.17.0",
    "typescript": "^3.5.2",
    "typescript-tslint-plugin": "^0.5.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.17.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.8",
    "@types/sqlite3": "^3.1.5",
    "@types/supertest": "^2.0.7",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
